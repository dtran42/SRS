@section Scripts{
    <script>
        $(document).ready(function () {
            $('.select2bs4').select2({
                theme: 'bootstrap4'
            })
        });
        $('.rm-date-picker').daterangepicker({
            singleDatePicker: true,
            autoUpdateInput: false
            
        });
        $('.rm-date-picker').on('apply.daterangepicker', function (ev, picker) {
            $(this).val(picker.startDate.format('MMMM D, YYYY'));
        });
        $("input[data-bootstrap-switch]").each(function () {
            $(this).bootstrapSwitch({
                onText: 'Yes',
                offText: 'No',
            });
            $(this).bootstrapSwitch('state', $(this).prop('checked'));
        });
        const Toast = Swal.mixin({
            toast: true,
            position: 'top-end',
            showConfirmButton: false,
            timer: 8000
        });
        var helpList = @Html.Raw(JsonConvert.SerializeObject(ViewBag.HelpList));
        $('.toastsDefaultDefault').click(function () {
            var key = this.id;
            var item = helpList.find(item => item.Key === key);
            $(document).Toasts('create', {
                title: item.Key,
                body: item.Value,
                autohide: true,
                delay: 8000,
            })
        });

        $(document).ready(function () {
            $('.custom-file-input').on('change', function () {
                var fileName = $(this).val().split('\\').pop();
                $(this).next('.custom-file-label').html(fileName);
            });
        });
    </script>
}
